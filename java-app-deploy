apiVersion: apps/v1
kind: Deployment
metadata:
  name: java-deployment      
  labels:
    app: backend
spec:
  replicas: 2                      
  selector:
    matchLabels:
      app: backend                 
  template:
    metadata:
      labels:
        app: backend
    spec:
      containers:
      - name: java-app
        image: ragavmuthukumar/javaspringboot  
        ports:
        - containerPort: 8080
